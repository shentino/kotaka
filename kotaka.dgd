telnet_port	= ([
	"*": 50000,		/* telnet login port */
	"*": 50001		/* status port */
]);		/* telnet port number */
binary_port	= ([
	"localhost": 50010,	/* kernel library backdoor */
	"*": 50011,		/* binary login port */
	"*": 50080		/* http */
]);		/* binary port number */
directory	= "Please read INSTALL";/* base directory (MUST be absolute) */
users		= 10;			/* max # of users */
editors		= 0;			/* max # of editor sessions */
ed_tmpfile	= "../tmp/ed";		/* proto editor tmpfile */
swap_file	= "../tmp/swap";	/* swap file */
swap_size	= 5000;			/* # sectors in swap file */
cache_size	= 50;			/* # sectors in swap cache */
sector_size	= 512;			/* swap sector size */
swap_fragment	= 32;			/* fragment to swap out */
static_chunk	= 64512;		/* static memory chunk */
dynamic_chunk	= 261120;		/* dynamic memory chunk */
dump_file	= "../snapshot";	/* snapshot file */
dump_interval	= 86400;		/* snapshot interval in seconds */

typechecking	= 2;			/* highest level of typechecking */
include_file	= "/include/std.h";	/* standard include file */
include_dirs	= ({ "/include", "~/include" }); /* directories to search */
auto_object	= "/kernel/lib/auto";	/* auto inherited object */
driver_object	= "/kernel/sys/driver";	/* driver object */
create		= "_F_create";		/* name of create function */

array_size	= 1000;			/* max array size */
objects		= 1000;			/* max # of objects */
call_outs	= 100;			/* max # of call_outs */
