telnet_port	= ([
	"*": 50001		/* status port */
]);		/* telnet port number */
binary_port	= ([
	"localhost": 50010,	/* kernel library backdoor */
	"*": 50000,		/* binary login port */
	"*": 50080		/* http */
]);		/* binary port number */
directory	= "/home/dgd/git/kotaka/mud";/* base directory (MUST be absolute) */
users		= 40;			/* max # of users */
