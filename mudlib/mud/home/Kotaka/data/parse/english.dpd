whitespace = / +/
prep = /at|in|on|from|by|with|of|for|to/
art = /a|an|the/
conj = /and|or/
word = /[^ ]+/

iclause: vp
iclause: vp ppl

vp: verb ? vp_verb
vp: verb np ? vp_verb_np

pp: prep ? pp_prep
pp: prep np ? pp_prep_np

np: oart oadjc noun ? np_oart_oadjc_noun

oart: ? oart
oart: art ? oart_art

oadjc: ? oadjc
oadjc: adjl ? oadjc_adjl

ppl: pp
ppl: ppl pp

adjl: adj
adjl: adjl adj

verb: word
noun: word
adj: word
adj: conj
